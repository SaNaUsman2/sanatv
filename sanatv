<script>
    
var search = document.getElementById("search");
var els = document.querySelectorAll("#el");

search.addEventListener("keyup", function() {

  Array.prototype.forEach.call(els, function(el) {
    if (el.textContent.toLowerCase().trim().indexOf(search.value.toLowerCase()) > -1)
      el.style.display = 'inline-block';
    else el.style.display = 'none';
  });

});

setInterval(function () {
  document.getElementById('splash').innerHTML = "SaNa.TV"; 
document.getElementById('popup').innerHTML =`<a href="" class="telegram-bu" id="wtext"><i class=""></i> 
</a>`;
document.getElementById('popup').style.backgroundColor=  "#25D366";



}, 1000);

setInterval(function () {
    
  document.getElementById('splash').innerHTML = "SaNa.TV"; 
  document.getElementById('wtext').innerHTML =` <a href=" class="" id="i"><i class="fab fa-telegram"></i> Join our channel
</a>`;
document.getElementById('popup').style.backgroundColor=  "#229ED9";

}, 3000);



function myMove() {
  let id = null;
  const elem = document.getElementById("popup");   
  let pos = 0;
  clearInterval(id);
  id = setInterval(frame, 1);
  function frame() {
    if (pos > 520) {
        
      pos++; 
            elem.style.marginbottom = pos + "px"; 
            elem.style.right = pos/3 +"px"; 
        
        //clearInterval(id);
    } else {
      pos++; 
      elem.style.top = pos + "px"; 
      elem.style.left = pos/3 +"px"; 

    }
  }
}
myMove();

</script>
